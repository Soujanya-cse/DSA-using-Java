class Solution {
    public int maxProfit(int[] prices) {
        
        int minPrice=prices[0];
        int profit=0;
        int temp=0;

        for(int i=1;i<prices.length;i++){
            if(prices[i]<minPrice){
                minPrice=prices[i];
            }

            else{
                temp=prices[i]-minPrice;
            }

            if(temp>profit){
                profit=temp;
            }
        }

        return profit;
    }
}
